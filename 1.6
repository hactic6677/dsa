#include<iostream>
#include<cmath>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		int a[n];
		
		for(int i=0;i<n;i++){
			a[i]=0;
		}
		for(int i=2;i<=sqrt(n);i++){
			while(n%i==0){
				a[i]++;
				n/=i;
			}
			if(a[i]!=0) cout<<i<<"("<<a[i]<<") ";
		}
		if(n>1) cout<<n<<"(1)"<<endl;
	}
}
